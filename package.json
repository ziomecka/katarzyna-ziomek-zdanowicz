  "description": "Katarzyna Ziomek Zdanowicz",
  "scripts": {
    "lint": "eslint . --ext .ts,.js",
    "setProduction": "./set-node-env.sh production",
    "setDevelopment": "./set-node-env.sh development",
    "nodemon": "nodemon --config nodemon.json",
    "nodemon:start": "node server/index.js",
    "tsc:server": "tsc -p ./tsconfig.json",
    "tsc:watch": "tsc --watch",
    "webpack:server": "webpack --config ./webpack/index.server.babel.js",
    "webpack:client": "webpack --config ./webpack/index.client.babel.js",
    "webpack:server:watch": "npm run setDevelopment && npm run webpack:server -- --watch",
    "webpack:client:watch": "npm run setDevelopment && npm run webpack:client -- --watch",
    "webpack:prod": "npm run setProduction && npm run webpack:server && npm run webpack:client",
    "bundle": "npm run tsc:server && npm run setServer && npm run webpack",
    "inspect": "node --inspect-brk node_modules/webpack/bin/webpack.js --config ./webpack/index.babel.js",
    "postinstall": "npm run webpack:client && npm run webpack:server ",
    "start": "node --optimize_for_size server/index.js $PORT"
  },
  "author": "Katarzyna Ziomek-Zdanowicz",
